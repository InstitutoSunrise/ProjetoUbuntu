{
  "expo": {
    "name": "ProjetoUbuntu",
    "slug": "react-native-ProjetoUbuntu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo_projeto.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo_projeto.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ]
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDokjQxsu7qomg4FUHtXP_UtqkJtiAd-dA"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
